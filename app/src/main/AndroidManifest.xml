<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team.sonemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sonemo">
        <activity android:name=".CreateGroup" />
        <activity android:name=".access.UserRegistrationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".CinemasActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".Messager" />
        <activity
            android:name=".MessagesActivity"
            android:theme="@style/Theme_Chat.Sonemo" />
        <activity android:name=".access.UserAccessActivity" />
    </application>

</manifest>